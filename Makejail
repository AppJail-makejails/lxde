INCLUDE options/options.makejail

ARG lxde_tag=13.2

FROM --entrypoint gh+AppJail-makejails/lxde lxde:${lxde_tag}

STOP

STAGE create

CMD --local xhost +

STAGE custom:lxde_open

ARG display=:0

ENV DISPLAY=${display}

USER lxde
RUN /usr/local/bin/ck-launch-session /usr/local/bin/dbus-launch startlxde
